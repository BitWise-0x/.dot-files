{
    "security.workspace.trust.untrustedFiles": "open",
    "editor.stickyScroll.enabled": true,
    "explorer.sortOrder": "type",
    "explorer.confirmDragAndDrop": false,
    "workbench.colorCustomizations": {
        "terminal.background": "#090300",
        "terminal.foreground": "#A5A2A2",
        "terminalCursor.foreground": "#A5A2A2",
        "terminal.ansiBlack": "#090300",
        "terminal.ansiBlue": "#01A0E4",
        "terminal.ansiBrightBlack": "#5C5855",
        "terminal.ansiBrightBlue": "#01A0E4",
        "terminal.ansiBrightCyan": "#B5E4F4",
        "terminal.ansiBrightGreen": "#01A252",
        "terminal.ansiBrightMagenta": "#A16A94",
        "terminal.ansiBrightRed": "#DB2D20",
        "terminal.ansiBrightWhite": "#F7F7F7",
        "terminal.ansiBrightYellow": "#FDED02",
        "terminal.ansiCyan": "#B5E4F4",
        "terminal.ansiGreen": "#01A252",
        "terminal.ansiMagenta": "#A16A94",
        "terminal.ansiRed": "#DB2D20",
        "terminal.ansiWhite": "#A5A2A2",
        "terminal.ansiYellow": "#FDED02",
    },
    "terminal.integrated.cursorBlinking": true,
    "editor.wordWrap": "on",
    "workbench.editor.highlightModifiedTabs": true,
    "explorer.compactFolders": false,
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "explorer.confirmDelete": false,
    "workbench.editorAssociations": {
        "*.odt": "default",
        "*.plist": "default"
    },
    "[python]": {
        "editor.formatOnType": true
    },
    "workbench.iconTheme": "vscode-icons",
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    // "terminal.integrated.fontFamily": "Fira Code",
    "terminal.integrated.fontFamily": "MesloLGS NF",

    "python.analysis.autoImportCompletions": true,
    "redhat.telemetry.enabled": true,
    "github.codespaces.defaultExtensions": [
        "GitHub.codespaces",
        "GitHub.vscode-pull-request-github"
    ],
    "github.codespaces.showPerformanceExplorer": true,
    "crypto-ticker.tickers": [

        {
            "symbol": "BTC",
            "currency": "USD",
            "exchange": "CCCAGG",
            "template": "{symbol} {price}"
        },
         {
            "symbol": "ETH"
        },
        {
            "symbol": "XRP"
        }
        ],
        "files.associations": {
            "Procfile": "procfile",
            "*.mdx": "markdown"
        },
        "[shellscript]": {
            "breadcrumbs.showClasses": true,
            "breadcrumbs.showConstants": true,
            "breadcrumbs.showArrays": true,
            "breadcrumbs.showFunctions": true,
            "breadcrumbs.showMethods": true,
            "breadcrumbs.showNull": true,
            "breadcrumbs.showFiles": true,
            "breadcrumbs.showFields": true,
            "breadcrumbs.showOperators": true,
            "breadcrumbs.showVariables": true,
            "files.eol": "\n"
        },
        "workbench.colorTheme": "GitHub Dark Default",
        "workbench.editor.splitInGroupLayout": "vertical",
        "editor.unicodeHighlight.invisibleCharacters": false,
        "terminal.integrated.env.osx": {

        },
        "editor.accessibilitySupport": "off",
        "github.copilot.enable": {
            "*": true,
            "plaintext": false,
            "markdown": true,
            "scminput": false
        },
        "editor.largeFileOptimizations": false,
        "vsicons.dontShowNewVersionMessage": true,
        "diffEditor.renderSideBySide": false,
        "git.ignoreRebaseWarning": true,
        "[yaml]": {
            "editor.insertSpaces": true,
            "editor.tabSize": 2,
            "editor.autoIndent": "keep",
            "diffEditor.ignoreTrimWhitespace": false,
            "editor.quickSuggestions": {
                "other": true,
                "comments": false,
                "strings": true
            }
        },
        "workbench.editor.empty.hint": "hidden",
        "diffEditor.maxComputationTime": 0,
        "githubPullRequests.pullBranch": "never",
        "accessibility.signals.lineHasError": {
            "sound": "off"
        },
        "accessibility.signals.terminalBell": {
            "sound": "on"
        },
        "terminal.integrated.enableVisualBell": true,
        "editor.minimap.enabled": false,
        "explorer.fileNesting.expand": false,
        "search.defaultViewMode": "tree",
        "scm.defaultViewMode": "tree",
        "githubPullRequests.createOnPublishBranch": "never",
        "go.toolsManagement.autoUpdate": true,
        "cmake.configureOnOpen": true,
        "workbench.startupEditor": "none",
        "diffEditor.ignoreTrimWhitespace": false,
        "markdown-pdf.outputDirectory": "~/Downloads/",
        "javascript.updateImportsOnFileMove.enabled": "always",
        "terminal.integrated.enableMultiLinePasteWarning": "never",

	"github.copilot.nextEditSuggestions.enabled": true,
	"github.copilot.chat.codesearch.enabled": true,
	"github.copilot.advanced": {


		"chatPreview": true,
		"modelSelection": true
	},
	"chat.mcp.assisted.nuget.enabled": true,
	"chat.mcp.autostart": "newAndOutdated",
	"chat.mcp.serverSampling": {
        "Global in Code: context7": {
            "allowedModels": [
                "copilot/gpt-5-experiment-b",
                "copilot/auto",
                "copilot/claude-3.5-sonnet",
                "copilot/claude-3.7-sonnet",
                "copilot/claude-3.7-sonnet-thought",
                "copilot/claude-sonnet-4",
                "copilot/claude-sonnet-4.5",
                "copilot/gemini-2.0-flash-001",
                "copilot/gemini-2.5-pro",
                "copilot/gpt-4.1",
                "copilot/gpt-4o",
                "copilot/gpt-5-mini",
                "copilot/gpt-5-codex",
                "copilot/grok-code-fast-1",
                "copilot/o3-mini",
                "copilot/o4-mini"
            ]
        },
        "Global in Code: deepwiki": {
            "allowedModels": [
                "copilot/claude-sonnet-4.5"
            ]
        }
    },
    "git.openRepositoryInParentFolders": "never",
    "workbench.secondarySideBar.defaultVisibility": "hidden",
    "[markdown]": {
        "diffEditor.ignoreTrimWhitespace": false
    },
    "chat.agent.maxRequests": 800,
    "[dockercompose]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.autoIndent": "advanced",
        "editor.quickSuggestions": {
            "other": true,
            "comments": false,
            "strings": true
        },
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "[github-actions-workflow]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "cron-explained.codeLens.enabled": true,
    "github.copilot.chat.alternateGptPrompt.enabled": true,
    "github.copilot.chat.languageContext.fix.typescript.enabled": true,
    "github.copilot.chat.languageContext.inline.typescript.enabled": true,
    "github.copilot.chat.languageContext.typescript.enabled": true,
    "chat.mcp.gallery.enabled": true,
    "terminal.integrated.suggest.enabled": true,
    "claudeCode.selectedModel": "default",
    "claudeCode.environmentVariables": [],
    "claudeCode.initialPermissionMode": "acceptEdits",
    "github.copilot.chat.anthropic.tools.websearch.enabled": true,
    "claudeCode.preferredLocation": "panel",
    "search.useIgnoreFiles": false,
    "chat.viewSessions.enabled": false
}
